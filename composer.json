{
    "name": "yurastrizhik/slack-error-notifier",
    "description": "Send a message to a Slack channel when error occurs in your Laravel Application, forked from freshbitsweb/slack-error-notifier",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*",
        "monolog/monolog": "~1.12"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gaurav Makhecha",
            "email": "info@freshbits.in"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "yurastrizhik\\SlackErrorNotifier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "yurastrizhik\\SlackErrorNotifier\\SlackErrorNotifierServiceProvider"
            ]
        }
    }
}
